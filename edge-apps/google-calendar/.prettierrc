{
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.mjs"],
      "options": {
        "singleQuote": true,
        "jsxSingleQuote": false,
        "semi": false,
        "trailingComma": "none"
      }
    },
    {
      "files": ["*.css"],
      "options": {
        "singleQuote": true
      }
    }
  ]
}
